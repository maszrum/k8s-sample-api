{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GeneralSettings": {
    "UseRedis": false,
    "RedisConnectionString": "localhost:6379",
    "UseConsul": false,
    "ConsulAddress": "http://localhost:8500",
    "ConsulSelfServiceScheme": "http",
    "ConsulSelfServiceAddress": "host.docker.internal",
    "ConsulSelfServicePort": 8080
  }
}
